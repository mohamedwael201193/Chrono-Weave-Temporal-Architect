{
  "name": "@privy-io/wagmi-connector",
  "version": "0.1.13",
  "keywords": [
    "authentication",
    "privy",
    "wagmi",
    "connector",
    "web3"
  ],
  "homepage": "https://github.com/orgs/privy-io/discussions",
  "bugs": {
    "url": "https://github.com/orgs/privy-io/discussions"
  },
  "description": "A connector for Privy <> WAGMI.sh",
  "module": "./dist/esm/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npx tsup --clean --minify",
    "watch": "npx tsup --watch",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\" && npx tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "rm -rf ./dist"
  },
  "author": "privy.io",
  "license": "Apache-2.0",
  "devDependencies": {
    "@saithodev/semantic-release-backmerge": "^2.2.0",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/node18": "^18.2.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "prettier": "^2.8.3",
    "semantic-release": "^21.0.7",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "@privy-io/react-auth": "^1.33.0",
    "react": "^18",
    "react-dom": "^18",
    "viem": ">=0.3.35",
    "wagmi": ">=1.4.12 <2"
  }
}
